{
  "name": "@frontal/style-guide",
  "description": "Frontal's engineering style guide",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "clean": "git clean -xdf -- node_modules .next .cache",
    "changeset": "changeset",
    "release": "changeset tag"
  },
  "files": ["biome.jsonc", "README.md", "config"],
  "exports": {
    "./angular.json": "./config/angular/biome.jsonc",
    "./astro.json": "./config/astro/biome.jsonc",
    "./biome.json": "./biome.jsonc",
    "./core.json": "./config/core/biome.jsonc",
    "./go.yml": "./config/go/golangci.yml",
    "./go/*": "./config/go/*",
    "./next.json": "./config/next/biome.jsonc",
    "./python.toml": "./config/python/ruff.toml",
    "./python/*": "./config/python/*",
    "./qwik.json": "./config/qwik/biome.jsonc",
    "./react.json": "./config/react/biome.jsonc",
    "./remix.json": "./config/remix/biome.jsonc",
    "./rust.toml": "./config/rust/rustfmt.toml",
    "./rust/*": "./config/rust/*",
    "./solid.json": "./config/solid/biome.jsonc",
    "./svelte.json": "./config/svelte/biome.jsonc",
    "./vue.json": "./config/vue/biome.jsonc"
  },
  "keywords": ["style", "guide", "format", "linter"],
  "dependencies": {
    "@biomejs/biome": "2.3.4"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7"
  }
}
